<!--=== Breadcrumbs ===-->
<div class="breadcrumbs">
    <div class="container">
        <h1 class="pull-left">@ViewBag.Title</h1>
        <ul class="pull-right breadcrumb">
            <li><a href="#">@ViewContext.RouteData.Values["controller"].ToString()</a></li>
            <li class="active">@ViewContext.RouteData.Values["action"].ToString()</li>
        </ul>
    </div>
</div><!--/breadcrumbs-->
<!--=== End Breadcrumbs ===-->